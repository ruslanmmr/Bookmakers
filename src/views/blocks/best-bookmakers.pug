        .table__row.table__row_head
          .table__col.table__col_title RANK
          .table__col.table__col_title BOOKIES RATING
          .table__col.table__col_title KEY FEATURES
          .table__col.table__col_title SIGN-UP BONUS
          .table__col.table__col_title CLAIM BONUS
        .table__row
          .table__col.col-rank
