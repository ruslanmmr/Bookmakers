                li.col-bonuses__icons-item
                  svg.icon <use xlink:href="img/icons/icon-sprite.svg#ico8"></use>
                li.col-bonuses__icons-item
                  svg.icon <use xlink:href="img/icons/icon-sprite.svg#ico7"></use>
            .col-bonuses__info-row
              .col-bonuses__rating
                svg.icon <use xlink:href="img/icons/icon-sprite.svg#ico27"></use>
                span.col-bonuses__rating-value
                  span 9.1
                  | /10
              .col-bonuses__comment
